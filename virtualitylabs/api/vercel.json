{
  "version": 2,
  "builds": [{
    "src": "sendemail.js",
    "use": "@now/node"
  }],
  "routes": [
    {"src": "/(.*)","dest": "sendemail.js"},
    {"src": "/api/images", "dest": "/api/images.js" },
    {"src": "/api/(.*)", "dest": "/api/otherRoutes.js" },
    { "src": "/api/sendEmail", "methods": ["POST"], "dest": "/sendEmail.js" }
  ]
}